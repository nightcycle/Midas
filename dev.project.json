{
	"name": "midas-test",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"Packages": {
				"$className": "Folder",
				"$path": "Packages",
				"Midas": {
					"$path": "src"
				}
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"Server": {
				"$path": "test/Server.server.luau"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"Client": {
					"$path": "test/Client.client.luau"
				}
			}
		}
	}
}
